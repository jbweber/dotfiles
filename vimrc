execute pathogen#infect()

syntax on

set t_Co=256
set background=dark

colorscheme Sunburst

set nocompatible
set nohlsearch
set ai
set noshowmatch
set ignorecase
set smartcase

set list

set tabstop=4
set shiftwidth=4
set softtabstop=4
set autoindent
set smartindent
set expandtab

filetype plugin indent on
